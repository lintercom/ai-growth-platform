{
  "name": "@aig/cli",
  "version": "0.1.0",
  "description": "CLI nÃ¡stroje pro AI Growth Platform",
  "type": "module",
  "bin": {
    "aig": "./dist/bin/aig.js",
    "aig-installer": "./dist/bin/aig-installer.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lintercom/ai-growth-platform.git"
  },
  "keywords": [
    "ai",
    "cli",
    "openai",
    "seo",
    "analysis"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "build:exe": "tsc && pkg dist/bin/aig-installer.js --targets node18-win-x64 --output-path ../bin --out-path ../bin"
  },
  "pkg": {
    "assets": [
      "../../node_modules/@aig/**/*"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "@aig/core": "workspace:*",
    "@aig/schemas": "workspace:*",
    "@aig/utils": "workspace:*",
    "@aig/workflows": "workspace:*",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
